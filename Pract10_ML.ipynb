   "source": [
    "s=input(\"Enter a String: \")\n",
    "s1=\"\"\n",
    "for i in s:\n",
    "    s1=i+s1\n",
    "if(s1==s):\n",
    "    print(s,\"is Palindrome\")\n",
    "else:\n",
    "    print(s,\"is not Palindrome\")"
